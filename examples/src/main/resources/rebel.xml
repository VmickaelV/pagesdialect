<?xml version="1.0" encoding="UTF-8"?>
<!-- 
    JRebel configuration. You can ignore it safely if you are not a JRebel user. 
    
    You must set up PAGESDIALECT_HOME system variable pointing to the PagesDialect root folder, as in
        -DPAGESDIALECT_HOME=/home/projects/thymeleaf/pagesdialect
-->
<application xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.zeroturnaround.com" xsi:schemaLocation="http://www.zeroturnaround.com http://www.zeroturnaround.com/alderaan/rebel-2_0.xsd">
    <classpath>
        <dir name="${PAGESDIALECT_HOME}/examples/target/classes"></dir>
        <dir name="${PAGESDIALECT_HOME}/core/target/classes"></dir>
    </classpath>
    <web>
        <link target="/">
            <dir name="${PAGESDIALECT_HOME}/examples/src/main/webapp"></dir>
        </link>
    </web>
</application>
